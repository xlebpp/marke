{
  "openapi": "3.0.1",
  "info": {
    "title": "marketplaceE",
    "version": "1.0"
  },
  "paths": {
    "/api/registr": {
      "post": {
        "tags": [
          "AuthControllers"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewUserDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/NewUserDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/NewUserDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/authentic": {
      "get": {
        "tags": [
          "AuthControllers"
        ],
        "parameters": [
          {
            "name": "Email",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "email"
            }
          },
          {
            "name": "PasswordHash",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NewUserDto": {
        "required": [
          "dateOfBirth",
          "email",
          "passwordHash",
          "role",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "passwordHash": {
            "minLength": 1,
            "type": "string"
          },
          "role": {
            "$ref": "#/components/schemas/RolesOfUsers"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date"
          }
        },
        "additionalProperties": false
      },
      "RolesOfUsers": {
        "enum": [
          "Master",
          "User"
        ],
        "type": "string"
      }
    }
  }
}